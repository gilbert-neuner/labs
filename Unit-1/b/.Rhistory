library(mobilizr)
load_lab(4)
load_lab(7)
?tally
data(titanic)
force(titanic)
View(titanic)
tally(~gender)
tally(~gender, data = titanic)
tally(~gender + survived, data = titanic)
tally(~gender | survived, data = titanic)
tally(survived~gender, data = titanic)
# 1b
con <- curl::curl("https://raw.githubusercontent.com/gilbert-neuner/labs/main/Unit-1/b/lab1b.html", "r")
page <- paste(readLines(con), collapse = '\n')
close(con)
# Create a temp HTML file
tf <- tempfile(fileext = ".html")
writeLines(page, tf)
# Display HTML file in the viewer pane.
rstudioapi::viewer(tf)
